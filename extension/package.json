{
    "name": "kanban-markdown",
    "displayName": "Kanban Markdown",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.91.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "kanban-markdown.helloWorld",
                "title": "Hello World"
            }
        ],
        "customEditors": [
            {
                "viewType": "kanban-markdown.editor",
                "displayName": "Kanban Markdown Editor",
                "selector": [
                    {
                        "filenamePattern": "*.kanban.md"
                    }
                ]
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "pnpm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/jquery": "^3.5.30",
        "@types/jqueryui": "^1.12.23",
        "@types/mocha": "^10.0.7",
        "@types/node": "20.x",
        "@types/vscode": "^1.91.0",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5"
    }
}
